    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.2",
    "uuid": "^3.3.2",
    "victory": "^31.3.0",
    "webpack": "^4.17.2"
  }
}
