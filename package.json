    "concurrently": "^4.0.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.7"
  },
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "axios": "^0.18.0",
    "babel-loader": "^8.0.2",
    "body-parser": "^1.20.1",
    "cors": "^2.8.4",
    "cross-env": "^5.2.0",
    "express": "^4.18.2",
    "history": "^4.7.2",
    "md5": "^2.2.1",
    "mongodb": "^3.1.10",
    "react": "^16.4.2",
    "react-dom": "^16.5.0",
