{
  "name": "react-express-full-stack-app-building",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "webpack -p",
    "deploy": "npm run build && git commit -a -m deployment && git push heroku master",
    "test": "echo \"Error: no test specified\" && exit 1",
    "old-start": "webpack",
    "start": "npm run build && npm run list-server && npm run server",
    "dev": "webpack-dev-server --open",
    "initialize": "babel-node src/server/initialize-db",
    "list-server": "ls -al ./node_modules//.bin/",
    "server": "./node_modules//.bin/babel-node src/server/server",
    "server-new": "cross-env NODE_ENV=production ./node_modules//.bin/babel-node src/server/server",
    "server-test": "babel-node src/server/server.spec",
    "start-dev": "concurrently \"npm run server\" \"npm run dev\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
